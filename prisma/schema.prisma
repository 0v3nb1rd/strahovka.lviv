// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("NEXT_DATABASE_URL")
}

model Service {
	id Int @default(autoincrement()) @id
	title String
	category String?
	category_en String?
	short_text String?
	full_text String?
	slug String @unique
	thumbnail_url String
	image_url String
	date DateTime?
}